<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">

    <class name="EntityC.Kid" table="Kid">
        <id name="certificateOfBorn" column="certificateofborn"></id>

        <property name="name" column="name"/>
        <property name="surname" column="surname"/>
        <property name="patronymic" column="patronymic"/>
        <property name="address" column="address"/>
        <property name="age" column="age"/>
        <property name="parentid1" column="parentid1"/>
        <property name="groupid" column="groupid"/>

    </class>
</hibernate-mapping>